{
  "realm": "TPDB",
  "enabled": true,
  "clients": [
    {
      "clientId": "tpdb-backend",
      "secret": "secret",
      "enabled": true,
      "bearerOnly": false,
      "publicClient": false,
      "directAccessGrantsEnabled": true,
      "protocol": "openid-connect"
    },
    {
      "clientId": "tpdb-frontend",
      "enabled": true,
      "publicClient": true,
      "protocol": "openid-connect",
      "redirectUris": ["http://localhost:4200/*"],
      "webOrigins": ["*"]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "ADMIN",
        "description": "Administrator role for TPDB"
      }
    ]
  },
  "users": [
    {
      "username": "adminuser",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Admin",
      "lastName": "User",
      "email": "admin@example.com",
      "credentials": [
        {
          "type": "password",
          "value": "adminpass",
          "temporary": false
        }
      ],
      "realmRoles": ["ADMIN"]
    }
  ]
}
